<?xml version="1.0" encoding="utf-8"?>
<SimulationInput xmlns="http://schemas.datacontract.org/2004/07/Vts.MonteCarlo" xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
  <DetectorInputs xmlns:a="http://schemas.microsoft.com/2003/10/Serialization/Arrays">
    <a:anyType i:type="ROfRhoDetectorInput">
      <Name>ROfRho</Name>
      <Rho xmlns:b="http://schemas.datacontract.org/2004/07/Vts.Common">
        <Count xmlns="http://schemas.datacontract.org/2004/07/Vts">101</Count>
        <Delta xmlns="http://schemas.datacontract.org/2004/07/Vts">0.1</Delta>
        <Start xmlns="http://schemas.datacontract.org/2004/07/Vts">0</Start>
        <Stop xmlns="http://schemas.datacontract.org/2004/07/Vts">10</Stop>
      </Rho>
      <TallyType>ROfRho</TallyType>
    </a:anyType>
  </DetectorInputs>
  <N>100</N>
  <Options>
    <AbsorptionWeightingType>Discrete</AbsorptionWeightingType>
    <PhaseFunctionType>HenyeyGreenstein</PhaseFunctionType>
    <RandomNumberGeneratorType>MersenneTwister</RandomNumberGeneratorType>
    <Seed>-1</Seed>
    <SimulationIndex>0</SimulationIndex>
    <TallySecondMoment>true</TallySecondMoment>
    <WriteDatabases xmlns:a="http://schemas.datacontract.org/2004/07/Vts">
      <a:DatabaseType>PhotonExitDataPoints</a:DatabaseType>
      <a:DatabaseType>CollisionInfo</a:DatabaseType>
    </WriteDatabases>
  </Options>
  <OutputName>infile_database_results</OutputName>
  <SourceInput i:type="CustomPointSourceInput">
    <a:PhiRange xmlns:b="http://schemas.datacontract.org/2004/07/Vts.Common" xmlns:a="http://schemas.datacontract.org/2004/07/Vts.MonteCarlo.Sources">
      <Count xmlns="http://schemas.datacontract.org/2004/07/Vts">1</Count>
      <Delta xmlns="http://schemas.datacontract.org/2004/07/Vts">0</Delta>
      <Start xmlns="http://schemas.datacontract.org/2004/07/Vts">0</Start>
      <Stop xmlns="http://schemas.datacontract.org/2004/07/Vts">0</Stop>
    </a:PhiRange>
    <a:PointLocation xmlns:b="http://schemas.datacontract.org/2004/07/Vts.Common" xmlns:a="http://schemas.datacontract.org/2004/07/Vts.MonteCarlo.Sources">
      <b:X>0</b:X>
      <b:Y>0</b:Y>
      <b:Z>0</b:Z>
    </a:PointLocation>
    <a:SolidAngleAxis xmlns:b="http://schemas.datacontract.org/2004/07/Vts.Common" xmlns:a="http://schemas.datacontract.org/2004/07/Vts.MonteCarlo.Sources">
      <b:Ux>0</b:Ux>
      <b:Uy>0</b:Uy>
      <b:Uz>1</b:Uz>
    </a:SolidAngleAxis>
    <a:ThetaRange xmlns:b="http://schemas.datacontract.org/2004/07/Vts.Common" xmlns:a="http://schemas.datacontract.org/2004/07/Vts.MonteCarlo.Sources">
      <Count xmlns="http://schemas.datacontract.org/2004/07/Vts">1</Count>
      <Delta xmlns="http://schemas.datacontract.org/2004/07/Vts">0</Delta>
      <Start xmlns="http://schemas.datacontract.org/2004/07/Vts">0</Start>
      <Stop xmlns="http://schemas.datacontract.org/2004/07/Vts">0</Stop>
    </a:ThetaRange>
  </SourceInput>
  <TissueInput i:type="MultiLayerTissueInput">
    <Regions xmlns:a="http://schemas.microsoft.com/2003/10/Serialization/Arrays">
      <a:anyType i:type="b:LayerRegion" xmlns:b="http://schemas.datacontract.org/2004/07/Vts.MonteCarlo.Tissues">
        <b:RegionOP xmlns:c="http://schemas.datacontract.org/2004/07/Vts">
          <c:G>0</c:G>
          <c:Mua>0</c:Mua>
          <c:Mus>1E-10</c:Mus>
          <c:Musp>1E-10</c:Musp>
          <c:N>1</c:N>
        </b:RegionOP>
        <b:ZRange xmlns:c="http://schemas.datacontract.org/2004/07/Vts.Common">
          <Count xmlns="http://schemas.datacontract.org/2004/07/Vts">2</Count>
          <Delta xmlns="http://schemas.datacontract.org/2004/07/Vts">INF</Delta>
          <Start xmlns="http://schemas.datacontract.org/2004/07/Vts">-INF</Start>
          <Stop xmlns="http://schemas.datacontract.org/2004/07/Vts">0</Stop>
        </b:ZRange>
      </a:anyType>
      <a:anyType i:type="b:LayerRegion" xmlns:b="http://schemas.datacontract.org/2004/07/Vts.MonteCarlo.Tissues">
        <b:RegionOP xmlns:c="http://schemas.datacontract.org/2004/07/Vts">
          <c:G>0.8</c:G>
          <c:Mua>0.01</c:Mua>
          <c:Mus>5.0000000000000009</c:Mus>
          <c:Musp>1</c:Musp>
          <c:N>1.4</c:N>
        </b:RegionOP>
        <b:ZRange xmlns:c="http://schemas.datacontract.org/2004/07/Vts.Common">
          <Count xmlns="http://schemas.datacontract.org/2004/07/Vts">2</Count>
          <Delta xmlns="http://schemas.datacontract.org/2004/07/Vts">100</Delta>
          <Start xmlns="http://schemas.datacontract.org/2004/07/Vts">0</Start>
          <Stop xmlns="http://schemas.datacontract.org/2004/07/Vts">100</Stop>
        </b:ZRange>
      </a:anyType>
      <a:anyType i:type="b:LayerRegion" xmlns:b="http://schemas.datacontract.org/2004/07/Vts.MonteCarlo.Tissues">
        <b:RegionOP xmlns:c="http://schemas.datacontract.org/2004/07/Vts">
          <c:G>0</c:G>
          <c:Mua>0</c:Mua>
          <c:Mus>1E-10</c:Mus>
          <c:Musp>1E-10</c:Musp>
          <c:N>1</c:N>
        </b:RegionOP>
        <b:ZRange xmlns:c="http://schemas.datacontract.org/2004/07/Vts.Common">
          <Count xmlns="http://schemas.datacontract.org/2004/07/Vts">2</Count>
          <Delta xmlns="http://schemas.datacontract.org/2004/07/Vts">INF</Delta>
          <Start xmlns="http://schemas.datacontract.org/2004/07/Vts">100</Start>
          <Stop xmlns="http://schemas.datacontract.org/2004/07/Vts">INF</Stop>
        </b:ZRange>
      </a:anyType>
    </Regions>
  </TissueInput>
</SimulationInput>